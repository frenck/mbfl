## ------------------------------------------------------------
## Setup.
## ------------------------------------------------------------

source [file join [file dirname [info script]] setup.tcl]

#PAGE
## ---------------------------------------------------------
## Test namespace.
## ---------------------------------------------------------

namespace eval ::test {

    variable	libstring [file join $libpath string.sh]

    variable	precmd ". %s"
    set precmd [format $precmd $libstring]


#PAGE

::tcltest::test libstring-1.1 "string chars" {
    -body	{
	bash "mbfl_string_chars abcdefghilm"
    }
    -result {a
b
c
d
e
f
g
h
i
l
m}
}

::tcltest::test libstring-1.2 "string chars" {
    -body	{
	bash {mbfl_string_chars "abcde fghilm"}
    }
    -result {a
b
c
d
e

f
g
h
i
l
m}
}

::tcltest::test libstring-1.3 "string chars" {
    -body	{
	bash "mbfl_string_chars \"abcdef\nghilm\""
    }
    -result {a
b
c
d
e
f

g
h
i
l
m}
}

